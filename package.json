{
  "name": "todolist",
  "version": "1.0.0",
  "description": "To Do List Desktop App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . ToDoList--overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . ToDoList --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ToDoList\"",
    "package-linux": "electron-packager . ToDoList --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luluzheng1/To-Do-List-.git"
  },
  "keywords": [
    "to",
    "do",
    "list"
  ],
  "author": "Wenlu Zheng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luluzheng1/To-Do-List-/issues"
  },
  "homepage": "https://github.com/luluzheng1/To-Do-List-#readme",
  "dependencies": {},
  "devDependencies": {
    "electron-packager": "^12.2.0",
    "electron": "^3.0.8"
  }
}
